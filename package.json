{
  "name": "component-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "commit": "git-cz"
  },
  "dependencies": {
    "@vue/composition-api": "^1.7.1",
    "core-js": "^3.8.3",
